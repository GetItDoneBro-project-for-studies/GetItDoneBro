{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "realm": "api",
    "auth-server-url": "https://your-keycloak-server.com/",
    "ssl-required": "external",
    "resource": "store-api",
    "verify-token-audience": true,
    "credentials": {
      "secret": ""
    },
    "confidential-port": 0,
    "Authority": "https://your-keycloak-server.com/realms/api",
    "Audience": "store-api",
    "RequireHttpsMetadata": true
  }
}
